  "stores": {
    "endclothing.com": {
      "name": "END. Clothing",
      "renderMode": "playwright",
      "waitTime": 3000,
      "imageSelectors": [
        "meta[property='og:image']",
        "img[data-testid='product-image']",
        "img[data-testid='main-image']",
        "picture source[type='image/webp']",
        ".product-image img",
        "[class*='ProductImage'] img",
        "[class*='gallery'] img",
        "[class*='Gallery'] img"
      ],
      "priceSelectors": [
        "[data-testid='product-price'] [data-testid='sale-price']",
        "[class*='SalePrice']",
        "[class*='sale-price']",
        ".product-price .price--sale",
        "meta[property='product:sale_price:amount']"
      ],
      "retailPriceSelectors": [
        "[data-testid='product-price'] [data-testid='original-price']",
        "[class*='OriginalPrice']",
        ".product-price .price--rrp",
        "meta[property='product:price:amount']"
      ],
      "nameSelectors": [
        "meta[property='og:title']",
        "h1[data-testid='product-name']",
        "h1[class*='ProductName']",
        ".product-name h1"
      ],
      "sizeSelectors": [
        "[data-testid='size-selector'] button",
        "[class*='SizeSelector'] button",
        "[class*='size-selector'] option",
        "select[name='size'] option"
      ],
      "imageUrlPattern": "media.endclothing.com"
    },

